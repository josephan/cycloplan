<h1>Create New Event</h1>
<div>
	<%= form_for(@event) do |f| %>
	  <%= render 'shared/error_messages', object: f.object %><br>
		<%= f.label :name %><br>
		<%= f.text_field :name %><br>
		<%= f.label :about %><br>
		<%= f.text_area :about, placeholder: "Not required" %><br>
		<%= f.submit %>
	<% end %>
</div>

<%= link_to "All custom events", events_path, class: "generic-button" %>